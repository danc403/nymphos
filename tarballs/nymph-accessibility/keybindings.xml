<keybind key="A-F4">
  <action name="Close"/>
</keybind>

<keybind key="S-A-F1">
  <action name="SendToDesktop">
    <to>1</to>
  </action>
</keybind>

<keybind key="S-A-F2">
  <action name="SendToDesktop">
    <to>2</to>
  </action>
</keybind>

<keybind key="S-A-F3">
  <action name="SendToDesktop">
    <to>3</to>
  </action>
</keybind>

<keybind key="S-A-F4">
  <action name="SendToDesktop">
    <to>4</to>
  </action>
</keybind>

<keybind key="C-S-A-Down">
  <action name="SendToDesktop">
    <to>down</to>
  </action>
</keybind>

<keybind key="C-S-A-Up">
  <action name="SendToDesktop">
    <to>up</to>
  </action>
</keybind>

<keybind key="C-A-Down">
  <action name="Desktop">
    <to>down</to>
  </action>
</keybind>

<keybind key="C-A-Up">
  <action name="Desktop">
    <to>up</to>
  </action>
</keybind>

<keybind key="W-d">
  <action name="ShowDesktop"/>
</keybind>

<keybind key="A-Tab">
  <action name="NextWindow"/>
</keybind>

<keybind key="W-Tab">
  <action name="NextWindow">
    <group>yes</group>
  </action>
</keybind>

<keybind key="A-Above_Tab">
  <action name="PreviousWindow">
    <group>yes</group>
  </action>
</keybind>

<keybind key="W-Left">
    <action name="SendToDesktop">
        <to>left</to>
    </action>
</keybind>

<keybind key="W-Right">
    <action name="SendToDesktop">
        <to>right</to>
    </action>
</keybind>

<keybind key="C-A-Left">
    <action name="Desktop">
        <to>left</to>
    </action>
</keybind>

<keybind key="C-A-Right">
    <action name="Desktop">
        <to>right</to>
    </action>
</keybind>

<keybind key="W-Home">
    <action name="Execute">
        <command>caja .</command>
    </action>
</keybind>

<keybind key="A-W-f">
    <action name="Execute">
        <command>firefox</command>
    </action>
</keybind>

<keybind key="C-A-t">
    <action name="Execute">
        <command>mate-terminal</command>
    </action>
</keybind>

<keybind key="A-W-Up">
    <action name="Execute">
        <command>wpctl set-volume @DEFAULT_AUDIO_SINK@ 5%+ && play /usr/share/sounds/freedesktop/stereo/audio-volume-change.oga</command>
    </action>
</keybind>

<keybind key="A-W-Down">
    <action name="Execute">
        <command>wpctl set-volume @DEFAULT_AUDIO_SINK@ 5%- && play /usr/share/sounds/freedesktop/stereo/audio-volume-change.oga</command>
    </action>
</keybind>

<keybind key="A-W-Left">
    <action name="Execute">
        <command>wpctl set-mute @DEFAULT_AUDIO_SINK@ toggle && play /usr/share/sounds/freedesktop/stereo/audio-volume-change.oga</command>
    </action>
</keybind>

<keybind key="A-W-o">
    <action name="Execute">
        <command>orca -r</command>
    </action>
</keybind>

<keybind key="A-W-s">
    <action name="Execute">
        <command>orca</command>
    </action>
</keybind>
