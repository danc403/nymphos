{
  "e2fsprogs.spec": {
    "name": "e2fsprogs",
    "version": "1.47.2",
    "release": "1%{?dist}",
    "summary": "Ext2/ext3/ext4 file system utilities",
    "License": "GPLv2+",
    "URL": "https://e2fsprogs.sourceforge.net/",
    "Source0": "%{name}-%{version}.tar.gz",
    "BuildRequires": [
      "autoconf",
      "automake",
      "bison",
      "flex",
      "gettext",
      "libtool",
      "pkg-config"
    ],
    "Requires": [
      "bash",
      "coreutils",
      "util-linux"
    ],
    "BuildRequires": "util-linux",
    "BuildRequires": "libuuid-devel",
    "Requires": "libuuid",
    "Provides": "mke2fs",
    "Provides": "fsck",
    "BuildRequires": "e2fsprogs-devel",
    "Requires": "e2fsprogs-libs",
    "Requires": "perl",
    "Provides": "debuginfo(rpmlint)",
    "Provides": "debuginfo(rpmlint_e2fsprogs)",
    "Provides": "debuginfo(findfs)",
    "Provides": "debuginfo(fsck.ext4)",
    "Provides": "debuginfo(mkfs)",
    "Provides": "debuginfo(resize)",
    "Provides": "debuginfo(tune)",
    "Provides": "debuginfo(tune2fs)",
    "Provides": "debuginfo(uuidgen)",
    "Provides": "debuginfo(uuidd)",
    "Provides": "debuginfo(e2fs)",
    "Provides": "debuginfo(e4defrag)",
    "Provides": "debuginfo(mke2fs.conf)",
    "Provides": "debuginfo(blkid)",
    "Provides": "debuginfo(dumpe2fs)",
    "Provides": "debuginfo(logsave)",
    "Provides": "debuginfo(badblocks)",
    "Provides": "debuginfo(debugfs)",
    "Provides": "debuginfo(e2label)",
    "Provides": "debuginfo(fsck.ext2)",
    "Provides": "debuginfo(fsck.ext3)",
    "Provides": "debuginfo(fsck.ext4)",
    "Provides": "debuginfo(mke2fs)",
    "Provides": "debuginfo(e2freefrag)",
    "Provides": "debuginfo(filefrag)",
    "Provides": "debuginfo(fsck)",
    "Provides": "debuginfo(fragcheck)",
    "Provides": "debuginfo(chattr)",
    "Provides": "debuginfo(lsattr)",
    "Provides": "debuginfo(e2image)",
    "Provides": "debuginfo(e2scrub)",
    "Provides": "debuginfo(e2undo)",
    "Provides": "debuginfo(replace)",
    "Provides": "debuginfo(fstrim)",
    "Provides": "debuginfo(zerofree)",
    "Provides": "debuginfo(profile)",
    "Provides": "debuginfo(add_overlayfs)",
    "Provides": "debuginfo(del_overlayfs)",
    "Provides": "debuginfo(e2fsck)",
    "Provides": "debuginfo(compile_et)",
    "Provides": "debuginfo(uuid)",
    "Provides": "debuginfo(libcom_err)",
    "Provides": "debuginfo(libss)",
    "Provides": "debuginfo(e2initr)",
    "Provides": "debuginfo(ss)",
    "Provides": "debuginfo(com_err)",
    "Provides": "debuginfo(uuid)" ,
    "BuildRoot": "%{_tmppath}/%{name}-%{version}-%{release}-root",
    "%description": "The e2fsprogs package contains the ext2fs filesystem utilities.",
    "%prep": "%setup -q",
    "%build": "%configure",
    "make %{?_smp_mflags}": null,
    "%install": "make install DESTDIR=%{buildroot}",
    "%files": [
      "/etc/mke2fs.conf",
      "/usr/sbin/*",
      "/usr/lib64/*",
      "/usr/share/man/man8/*",
      "/usr/share/man/man5/*",
      "/usr/bin/*",
      "/usr/share/doc/%{name}/*",
      "/usr/lib64/lib*.so.*"
    ],
    "%changelog": [
      "* Tue Oct 24 2023 Dan Carpenter DanC403@gmail.com - 1.47.2-1",
      "- Initial build"
    ]
  },
  "e2fsprogs-devel.spec": {
    "name": "e2fsprogs-devel",
    "version": "1.47.2",
    "release": "1%{?dist}",
    "summary": "Development files for e2fsprogs",
    "License": "GPLv2+",
    "URL": "https://e2fsprogs.sourceforge.net/",
    "Source0": "%{name}-%{version}.tar.gz",
    "Requires": "e2fsprogs = %{version}-%{release}",
    "Requires": "libuuid-devel",
    "BuildRequires": "e2fsprogs",
    "BuildRequires": "libuuid-devel",
    "BuildRoot": "%{_tmppath}/%{name}-%{version}-%{release}-root",
    "%description": "This package contains the header files and libraries needed to develop programs that use the e2fsprogs library.",
    "%prep": "%setup -q",
    "%build": "%configure",
    "make %{?_smp_mflags}": null,
    "%install": "make install DESTDIR=%{buildroot}",
    "%files": [
      "/usr/include/*",
      "/usr/lib64/lib*.so",
      "/usr/lib64/pkgconfig/*"
    ],
    "%changelog": [
      "* Tue Oct 24 2023 Dan Carpenter DanC403@gmail.com - 1.47.2-1",
      "- Initial build"
    ]
  }
}
